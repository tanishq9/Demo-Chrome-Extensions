{
    "name" : "Bear",
    "manifest_version" : 2,
    "version" : "1.0.0",
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["content.js"]
        }
    ],
    "browser_action" : {
        "default_popup" : "popup.html",
        "default_title" : "Bear"
    }
}