{
    "name" : "Bear Multiple",
    "manifest_version" : 2,
    "version" : "1.0.0",
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js" : ["content.js"]
        }
    ],
    "browser_action" : {
        "default_title" : "Bear Multiple"
    },
    "background" : {
        "scripts" : ["background.js"]
    },
    "permissions" : ["tabs"]
}